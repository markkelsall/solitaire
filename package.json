{
  "name": "simple-react-full-stack",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/client/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "http-server",
    "dev-client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "dev-server": "nodemon --exec babel-node src/server/index.js",
    "dev": "concurrently \"npm run dev-server\" \"npm run dev-client\"",
    "test": "jest --coverage"
  },
  "author": "Mark Kelsall",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "http-server": "^0.11.1",
    "prop-types": "^15.6.2",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-hot-loader": "4.12.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "8",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^3.0.0",
    "concurrently": "^3.5.1",
    "css-loader": "^1.0.1",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "4.12.0",
    "nodemon": "^1.19.2",
    "react-test-renderer": "^16.9.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.40.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.1.3"
  }
